{
  "host": "host",
  "port": 1111,
  "transport": "UDP",
  "secure": false,
  "app": "application",
  "dryrun": false,
  "sampleRate": 100,
  "namespace": "namespace",
  "timerAggregations": [
    "AVG",
    "COUNT"
  ],
  "timerFrequency": "FREQ_10",
  "enabled": true,
  "http-server-url-patterns": [
    {
      "pattern": "\\w{8}-\\w{4}-\\w{4}-\\w{4}-\\w{12}",
      "replacement": "_uuid_"
    }
  ],
  "http-server-ignore-url-patterns": [
    "/static/.*"
  ],
  "collectors": {
    "pool": true,
    "httpClient": true,
    "httpServer": true
  }
}